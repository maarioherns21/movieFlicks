<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>movies</title>
</head>
<body>
   <% movies.forEach(function(m) { %> 
   
    <image src= "<%= m.url %> " width="400" height="450">
        <h2><%= m.movieName %> </h2>
   
        <a href="/movies/<%= m._id %>"><button type="button" class="button">Details</button></a>
    <% }) ; %> 
</body>
</html>